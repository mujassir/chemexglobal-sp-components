{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "up-coming-events-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/upComingEvents/UpComingEventsWebPart.js",
          "manifest": "./src/webparts/upComingEvents/UpComingEventsWebPart.manifest.json"
        }
      ]
    },
    "employee-spotlight-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/employeeSpotlight/EmployeeSpotlightWebPart.js",
          "manifest": "./src/webparts/employeeSpotlight/EmployeeSpotlightWebPart.manifest.json"
        }
      ]
    },
    "whats-new-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/whatsNew/WhatsNewWebPart.js",
          "manifest": "./src/webparts/whatsNew/WhatsNewWebPart.manifest.json"
        }
      ]
    },
    "icon-widget-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/iconWidget/IconWidgetWebPart.js",
          "manifest": "./src/webparts/iconWidget/IconWidgetWebPart.manifest.json"
        }
      ]
    },
    "slider-widget-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/sliderWidget/SliderWidgetWebPart.js",
          "manifest": "./src/webparts/sliderWidget/SliderWidgetWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {},
  "localizedResources": {
    "UpComingEventsWebPartStrings": "lib/webparts/upComingEvents/loc/{locale}.js",
    "EmployeeSpotlightWebPartStrings": "lib/webparts/employeeSpotlight/loc/{locale}.js",
    "WhatsNewWebPartStrings": "lib/webparts/whatsNew/loc/{locale}.js",
    "IconWidgetWebPartStrings": "lib/webparts/iconWidget/loc/{locale}.js",
    "SliderWidgetWebPartStrings": "lib/webparts/sliderWidget/loc/{locale}.js",
    "ControlStrings": "node_modules/@pnp/spfx-controls-react/lib/loc/{locale}.js"
  }
}